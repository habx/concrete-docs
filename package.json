{
  "name": "concrete-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus start",
    "build": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus build",
    "swizzle": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus swizzle",
    "deploy": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus deploy",
    "clear": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus clear",
    "serve": "BROWSERSLIST_DANGEROUS_EXTEND=1 docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint \"docs/**\" \"src/**\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/client-projects.git"
  },
  "author": "HABX Front End team",
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.72",
    "@docusaurus/preset-classic": "2.0.0-alpha.72",
    "@habx/ui-core": "^5.36.1",
    "@habx/ui-table": "^6.7.3",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-medium-image-zoom": "4.3.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.72",
    "@habx/config-ci-front": "^0.7.0",
    "@habx/eslint-config-client": "^6.0.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "eslint-plugin-mdx": "^1.11.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    "extends @habx/config-ci-front/browserslist"
  ],
  "config-overrides-path": "node_modules/@habx/config-ci-front/webpack/overrides"
}
